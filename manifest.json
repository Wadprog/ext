{
	"manifest_version": 2,
	"name": "Invoice",
	"version": "0.1.0",
	"icons": { "1": "icon.png" },
	"background": {
		"scripts": [ "background.js" ]
	},
	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": [ "script.js" ],
			"css": [ "style.css" ]
		}
	],
	"permissions": [ "tabs", "webNavigation" ]
}
